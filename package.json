{
  "name": "colorcove",
  "private": true,
  "description": "A site for filmmakers and creatives built with Gatsby",
  "version": "0.1.0",
  "author": "Leigh Kendell <leigh@leighkendell.com>",
  "dependencies": {
    "@sanity/client": "^0.140.36",
