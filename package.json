    "@storybook/addon-centered": "^5.1.9",
    "@storybook/addon-info": "^5.1.9",
    "@storybook/addon-links": "^5.1.9",
    "@storybook/addon-viewport": "^5.1.9",
    "@storybook/addons": "^5.1.9",
    "@storybook/react": "^5.1.9",
    "@svgr/webpack": "^4.3.0",
    "@types/react-helmet": "^5.0.8",
    "@types/storybook__react": "^4.0.2",
    "@types/styled-components": "^4.1.16",
    "@types/vimeo__player": "^2.6.2",
    "@typescript-eslint/eslint-plugin": "^1.10.2",
    "@typescript-eslint/parser": "^1.10.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-typescript-to-proptypes": "^0.17.1",
    "eslint-config-react-app": "^4.0.1",
    "eslint-plugin-react-hooks": "^1.6.0",
    "husky": "^2.4.1",
    "lint-staged": "^8.2.1",
    "netlify-lambda": "^1.4.13",
    "prettier": "^1.18.2",
    "stylelint": "^10.1.0",
    "stylelint-8-point-grid": "^0.2.2",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recommended": "^2.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^3.0.0",
    "stylelint-processor-styled-components": "^1.8.0",
    "typescript": "^3.5.2"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "yarn run check-ts && lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint",
      "stylelint",
      "prettier --write",
      "git add"
    ]
  },
  "scripts": {
    "build": "gatsby build",
    "build:lambda": "netlify-lambda build src/lambda",
    "develop": "gatsby develop",
    "develop:lambda": "netlify-lambda serve src/lambda",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "check-ts": "tsc -p tsconfig.json --noEmit",
    "generate-graphql-types": "graphql-codegen --config codegen.yml"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/leighkendell/colorcove"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
